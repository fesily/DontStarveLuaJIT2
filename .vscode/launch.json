{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach game by cpp",
            "type": "cppvsdbg",
            "request": "attach",
            "visualizerFile": "${workspaceFolder}/LuaJit.Natvis"
        },
        {
            "name": "(Windows) 启动",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "E:/SteamLibrary/steamapps/common/Don't Starve Together/bin64/dontstarve_steam_x64.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "E:/SteamLibrary/steamapps/common/Don't Starve Together",
            "environment": []
        },
        {
            "name": "Launch luajit",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/luajit/src/luajit.exe",
            "args": [
                "test.lua"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal",
            "visualizerFile": "${workspaceFolder}/LuaJit.Natvis"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
    ]
}